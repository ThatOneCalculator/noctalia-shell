{
  "languages": {
    "QML": {
      "format_on_save": "on",
      "formatter": {
        "external": {
          "command": "qmlfmt",
          "arguments": [
            "-e",
            "-b",
            "360",
            "-t",
            "2",
            "-i",
            "2",
            "{buffer_path}"
          ]
        }
      }
    }
  }
}
